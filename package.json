{
  "name": "skillswap-mvp",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "author": "Paul C. Schmidt",
  "copyright": "Copyright Paul C. Schmidt 2025. All rights reserved. Unauthorized use, reproduction, or distribution of this software is prohibited.",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ui": "jest --testPathPattern=tests/components/ui",
    "test:lib": "jest --testPathPattern=tests/lib",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:badges": "jest --coverage && jest-coverage-badges",
    "test:updateSnapshots": "jest --updateSnapshot",
    "test:coverage:report": "node scripts/run-tests-with-coverage.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "cypress run --headless",
    "cypress:chrome": "cypress run --browser chrome",
    "cypress:firefox": "cypress run --browser firefox",
    "cypress:registration": "cypress run --spec 'cypress/e2e/registration.cy.js'",
    "cypress:skill-search": "cypress run --spec 'cypress/e2e/skill-search.cy.js'",
    "cypress:match-flow": "cypress run --spec 'cypress/e2e/match-flow.cy.js'",
    "cypress:skill-management": "cypress run --spec 'cypress/e2e/skill-management.cy.js'",
    "cypress:report": "cypress run --reporter mochawesome",
    "test:e2e": "npm run cypress:run",
    "test:e2e:chrome": "npm run cypress:chrome",
    "test:e2e:firefox": "npm run cypress:firefox",
    "prepare-env": "node scripts/env-manager.js",
    "update-env:staging": "node scripts/update-env-variables.js staging",
    "update-env:production": "node scripts/update-env-variables.js production",
    "prebuild:verify": "next build",
    "validate-deps": "node scripts/validate-dependencies.js",
    "deploy:staging": "npm run validate-deps && npm run update-env:staging && npm run prebuild:verify && vercel --prod",
    "deploy:production": "npm run validate-deps && npm run update-env:production && npm run prebuild:verify && vercel --prod",
    "verify-deployment": "node scripts/verify-deployment.js",
    "deploy": "node deploy.js",
    "deploy:full:staging": "node deploy.js staging",
    "deploy:full:production": "node deploy.js production",
    "lint:fix": "eslint --fix ./src --config .eslintrc.cjs",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env ANALYZE=true BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env ANALYZE=true BUNDLE_ANALYZE=browser next build",
    "update-imports": "node scripts/update-imports.js",
    "build:vercel": "node scripts/update-imports.js && yarn run build"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^0.1.5",
    "@heroicons/react": "^2.0.18",
    "@sentry/nextjs": "^9.18.0",
    "@supabase/auth-helpers-nextjs": "^0.8.5",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "classnames": "^2.3.2",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "localforage": "^1.10.0",
    "lucide-react": "^0.509.0",
    "next": "14.0.2",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "swr": "^2.3.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@next/bundle-analyzer": "^15.3.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "autoprefixer": "^10.0.1",
    "axe-core": "^4.10.3",
    "cross-env": "^7.0.3",
    "cypress": "^13.5.0",
    "cypress-axe": "^1.6.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "postcss": "^8",
    "react-query": "^3.39.3",
    "tailwindcss": "^3.3.0",
    "typescript": "5.0.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
